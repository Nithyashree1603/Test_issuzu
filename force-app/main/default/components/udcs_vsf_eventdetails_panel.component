            <img src="{!URLFOR($Resource.udcs_connectx,'/icons/common/up_arrow/up_arrow_dark_gray.svg')}" />
          </div>
        </div>
      </div>
      <div id="evt_dataview">
        <p class="dataview-label">{!$Label.ud_dataview}</p>
      </div>
    </div>
