      <div class="back_button_evt_details" id="back_button">
        <img style="cursor: pointer" src="{!URLFOR($Resource.udcs_connectx,'/icons/common/left_arrow.svg')}" width="15" height="15" class="left_arrow_back" /> {!$Label.ud_back}
      </div>
      <p>{!$Label.ud_event_details}</p>
      <div id="vehicleDetailsclose" class="closeIcon">
        <img class="location_icon" style="cursor: pointer" src="{!URLFOR($Resource.udcs_connectx,'/icons/common/location.svg')}" width="28" height="28" />
      </div>
    </div>
    <div class="row11">
      <div class="evt_header_container">
        <div class="first-head">
          <div class="event_nxt" id="event_nxt">
            <img src="{!URLFOR($Resource.udcs_connectx,'/icons/common/left_arrow/left_arrow_dark_gray.svg')}" />
          </div>
          <div id="eventtitle" class="eventtitle"></div>
          <div id="event_prv" class="event_prv">
